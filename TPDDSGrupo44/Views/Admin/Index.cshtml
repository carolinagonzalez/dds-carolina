@model IEnumerable<TPDDSGrupo44.Models.Busqueda>

@{
    ViewBag.Title = "SearchReports";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Search Reports</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.textoBuscado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cantidadDeResultados)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.textoBuscado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cantidadDeResultados)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha)
        </td>
    </tr>
}

</table>
