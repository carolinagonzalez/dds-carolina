        <td>
            @Html.DisplayFor(modelItem => item.numeroDeComuna)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.zonaDelimitadaPorLaComuna)
        </td>
        <td>
            @Html.ActionLink("Editar", "EditCGP", new { id=item.id }) |
            @Html.ActionLink("Borrar", "DeleteCGP", new { id=item.id })
        </td>
    </tr>
}

</table>
