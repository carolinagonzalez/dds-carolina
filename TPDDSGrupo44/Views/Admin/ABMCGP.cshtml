@model IEnumerable<TPDDSGrupo44.Models.CGP>

@{
    ViewBag.Title = "ABMCGP";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ABMCGP</h2>

<p>
    @Html.ActionLink("Agregar CGP", "CreateCGP")
</p>
<table class="table">
